<div id="wrap">
  <div id="main" class="container clear-top">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Email</th>
          <th>Room No</th>
          <th>Start Date</th>
          <th>Last Date</th>
          <th>Guest</th>
          <th> Show <th>
        </tr>
      </thead>
      <% @bookings.each do |booking|%>
        <tr>
          <td><%= booking.user.email %></td>
          <td><%= link_to booking.room.room_no, booking %> </td>
          <td><%= booking.start_date %></td>
          <td><%= booking.last_date %></td>
          <td><%= booking.number_of_person %></td>
          <td> <%= link_to "Show", booking %> </td>
        </tr>  
      <%end%>
    </table>
    <%= will_paginate @bookings %>
  </div>
</div>
